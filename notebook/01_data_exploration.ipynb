    "\n",
    "# Custom imports\n",
    "from data.data_loader import MedicalImageDataLoader\n",
    "from utils.visualization import visualize_sample_images, visualize_data_distribution\n",
    "\n",
    "# Set style\n",
    "plt.style.use('seaborn-v0_8')\n",
    "sns.set_palette('husl')\n",
    "%matplotlib inline"
